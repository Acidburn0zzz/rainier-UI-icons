[Icon Theme]
Name=Rainier UI Icons
Comment=Crisp, modern icon set for Trenta OS by Trenta.io
Inherits=Numix-Circle

Example=rainier-icon-theme

# Directory list
Directories=64x64/devices,64x64/status,128x128/apps,256x256/apps,256x256/places,256x256/status

# Actions

[64x64/devices]
Context=Devices
Size=64
Type=Scalable
MaxSize=64
MinSize=4

[64x64/status]
Context=Status
Size=64
Type=Scalable
MaxSize=64
MinSize=4

[128x128/apps]
Context=Applications
Size=128
Type=Scalable
MaxSize=128
MinSize=4

