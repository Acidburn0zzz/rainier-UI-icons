[256x256/apps]
Context=Applications
Size=256
Type=Scalable
MaxSize=2048
MinSize=4

[256x256/places]
Context=Places
Size=256
Type=Scalable
MaxSize=2048
MinSize=4

[256x256/status]
Context=Status
Size=256
Type=Scalable
MaxSize=2048
MinSize=4

