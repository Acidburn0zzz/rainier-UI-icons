Size=256
Type=Threshold

[256x256/places]
Context=Places
Size=256
Type=Threshold

[22x22/status]
Size=22
Context=Status
Type=Threshold

[24x24/status]
Size=24
Context=Status
Type=Fixed

[32x32/status]
Size=32
Context=Status
Type=Fixed

[32x32/actions]
Size=32
Context=Actions
Type=Scalable

[48x48/status]
Size=128
Context=Status
Type=Scalable
