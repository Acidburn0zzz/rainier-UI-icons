[Icon Theme]
Name=Rainier
Comment=Crisp, modern icon set for Trenta OS.
Inherits=Numix-Circle

Example=rainier-icon-theme

# Directory list
Directories=256x256/apps,256x256/places,

# Actions

[256x256/apps]
