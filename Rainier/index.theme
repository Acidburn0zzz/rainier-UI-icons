[Icon Theme]
Name=Rainier
Comment=Crisp, modern icon set for Trenta OS.
Inherits=Numix-Circle
# Licensed Under GPLv3

Example=x-directory-normal

# Directory list
Directories=22x22/status,24x24/status,32x32/status,32x32/actions,48x48/actions,48x48/status,256x256/apps,256x256/places,

